{
  "name":"dunnovc-staging",
  "scripts":{
    "postdeploy": "bundle exec rake db:schema:load db:seed"
  },
  "env":{
    "RACK_ENV":{
      "required":true
    },
    "RAILS_ENV":{
      "required":true
    },
    "SECRET_TOKEN":{
      "required":true
    },
    "WEB_CONCURRENCY":{
      "required":true
    },
    "AWS_ACCESS_KEY_ID":{
      "required":true
    },
    "AWS_SECRET_ACCESS_KEY":{
      "required":true
    },
    "AWS_BUCKET_NAME":{
      "required":true
    },
    "REDIS_PROVIDER":{
      "required":true
    },
    "SEARCHBOX_SSL_URL":{
      "required":true
    },
    "ELASTICSEARCH_URL":{
      "required":true
    },
    "ERRBIT_API_KEY":{
      "required":true
    },
    "GA":{
      "required":true
    }
  },
  "addons":[
    "memcachier",
    "redistogo",
    "searchbox",
    "sendgrid",
    "heroku-postgresql"
  ]
}
