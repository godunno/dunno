AWSCredentials = ($http) ->
  $http.get('/api/v1/utils/s3/credentials').then (response)=>
    credentials = response.data
    @accessKeyId = credentials.access_key
    @policy = credentials.policy
    @signature = credentials.signature
    @baseUrl = credentials.base_url
  @

AWSCredentials.$inject = ['$http']

angular
  .module('app.core')
  .service('AWSCredentials', AWSCredentials)
