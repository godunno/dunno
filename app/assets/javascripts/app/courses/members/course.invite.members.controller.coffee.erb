InviteMembersController = ($modal, $modalInstance) ->
  @openInviteMembersFullscreen = ->
    $modal.open
      templateUrl: '<%= asset_path('courses/_invite_members_fullscreen.html') %>'
    $modalInstance.close()

  @

InviteMembersController.$inject = ['$modal', '$modalInstance']

angular
  .module('DunnoApp')
  .controller('InviteMembersController', InviteMembersController)
