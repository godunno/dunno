InviteMembersFullscreenController = (modalInstance, course) ->
  @course = course
  @close = ->
    modalInstance.destroy()

  @

InviteMembersFullscreenController.$inject = ['modalInstance', 'course']

angular
  .module('app.courses')
  .controller('InviteMembersFullscreenController', InviteMembersFullscreenController)
