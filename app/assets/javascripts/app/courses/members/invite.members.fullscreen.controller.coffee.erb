InviteMembersFullscreenController = ($modalStack, course) ->
  @course = course
  @close = ->
    $modalStack.dismissAll()

  @

InviteMembersFullscreenController.$inject = ['$modalStack', 'course']

angular
  .module('app.courses')
  .controller('InviteMembersFullscreenController', InviteMembersFullscreenController)
