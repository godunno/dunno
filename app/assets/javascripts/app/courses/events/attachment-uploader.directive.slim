.attachment-uploader
  a.button.send-file
    | Enviar arquivo
  input.file-select.hide type="file" ngf-multiple="true" ngf-select="vm.upload($files)"
  attachment[
    ng-model="file"
    on-abort="vm.removeFile"
    on-create="vm.attachmentCreated"
    on-delete="vm.attachmentDeleted"
    ng-repeat="file in vm.files"]
